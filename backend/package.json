    "dotenv": "^16.0.1",
    "joi": "^17.6.0",
    "marked": "^4.0.18",
    "mysql2": "^2.3.3",
    "node-fetch": "2.6.7",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.6",
    "typeorm": "^0.3.7",
