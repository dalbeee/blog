  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "yarn migration:run && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:unit": "jest --config ./jest.config.unit.ts",
    "test:esm": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config jest.config.unit.esm.ts --runInBand",
    "test:unit:watch": "yarn test:unit --watchAll",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./jest.config.e2e.ts",
    "test:e2e:watch": "yarn test:e2e --watchAll",
    "typeorm": "typeorm-ts-node-commonjs -d ./migration/dataSource.ts",
    "migration:create": "yarn typeorm migration:create",
    "migration:generate": "yarn typeorm migration:generate",
    "migration:run": "yarn typeorm migration:run",
    "migration:revert": "yarn typeorm migration:revert"
  },
  "dependencies": {
